<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    android:paddingBottom="30dp"
    tools:context=".LoginActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:elevation="10dp"
            app:cardCornerRadius="20dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_one">


                <EditText
                    android:id="@+id/et_company_address"
                    android:layout_width="350dp"
                    android:layout_height="150dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="70dp"
                    android:background="@drawable/main_layout_background"
                    android:gravity="start"
                    android:hint="@string/prompt_company_address"
                    android:inputType="text"
                    android:paddingLeft="20dp"
                    android:paddingTop="20dp"
                    android:paddingRight="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="20sp" />

                <EditText
                    android:id="@+id/et_license_number"
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/et_company_address"
                    android:layout_alignStart="@id/et_company_address"
                    android:layout_marginTop="10dp"
                    android:drawableLeft="@drawable/ic_id_icon"
                    android:drawablePadding="10dp"
                    android:hint="@string/prompt_license_number"
                    android:inputType="text"
                    android:paddingLeft="20dp"
                    android:paddingRight="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_license_image_add_button"
                    android:layout_width="180dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/et_license_number"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/view_background"
                    android:drawableTint="@color/black"
                    android:gravity="center"
                    android:paddingLeft="10dp"
                    android:text="Add license image"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv_profile_image_add_button"
                    android:layout_width="180dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/et_license_number"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="3dp"
                    android:layout_toRightOf="@id/tv_license_image_add_button"
                    android:background="@drawable/view_background"
                    android:drawableTint="@color/black"
                    android:gravity="center"
                    android:paddingLeft="10dp"
                    android:text="Add profile image"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/iv_display_license_image"
                    android:layout_width="350dp"
                    android:layout_height="400dp"
                    android:layout_below="@id/tv_license_image_add_button"
                    android:layout_centerHorizontal="true"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/iv_display_profile_image"
                    android:layout_width="350dp"
                    android:layout_height="400dp"
                    android:layout_below="@id/iv_display_license_image"
                    android:layout_centerHorizontal="true"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_user_kit"
                    android:layout_width="250dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/iv_display_license_image"
                    android:layout_alignStart="@id/et_license_number"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="20dp"
                    android:gravity="center_vertical"
                    android:text="@string/query_user_kit"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <com.google.android.material.switchmaterial.SwitchMaterial
                    android:id="@+id/sw_user_kit_switch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/et_license_number"
                    android:layout_alignTop="@id/tv_user_kit"
                    android:layout_alignBottom="@id/tv_user_kit"
                    android:layout_marginLeft="20dp"
                    android:layout_toRightOf="@id/tv_user_kit" />

                <TextView
                    android:id="@+id/tv_user_kit_description"
                    android:layout_width="350dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/tv_user_kit"
                    android:layout_alignStart="@id/tv_user_kit"
                    android:layout_alignEnd="@id/sw_user_kit_switch"
                    android:layout_marginStart="-2dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="2dp"
                    android:text="@string/description_user_kit"
                    android:textColor="@color/black"
                    android:textSize="15sp"
                    android:textStyle="italic" />

                <TextView
                    android:id="@+id/tv_bank_details_heading"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/tv_user_kit_description"
                    android:layout_alignStart="@id/et_license_number"
                    android:layout_marginTop="5dp"
                    android:text="@string/bank_details_heading"
                    android:textColor="@color/text_color"
                    android:textSize="25sp"
                    android:textStyle="bold" />


                <EditText
                    android:id="@+id/et_account_number"
                    android:layout_width="350dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/tv_bank_details_heading"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:autofillHints="text"
                    android:drawableLeft="@drawable/bank"
                    android:drawablePadding="10dp"
                    android:hint="@string/prompt_account_number"
                    android:inputType="text"
                    android:paddingLeft="20dp"
                    android:paddingRight="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="20sp" />


                <EditText
                    android:id="@+id/et_branch_address"
                    android:layout_width="350dp"
                    android:layout_height="150dp"
                    android:layout_below="@id/et_account_number"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/main_layout_background"
                    android:gravity="start"
                    android:hint="@string/prompt_bank_address"
                    android:inputType="text"
                    android:paddingLeft="20dp"
                    android:paddingTop="20dp"
                    android:paddingRight="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="20sp" />

                <EditText
                    android:id="@+id/et_IFSC_code"
                    android:layout_width="350dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/et_branch_address"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:drawableLeft="@drawable/number"
                    android:drawablePadding="10dp"
                    android:hint="@string/IFSC_code"
                    android:inputType="text"
                    android:paddingLeft="20dp"
                    android:paddingRight="10dp"
                    android:textColor="@color/text_color"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/bt_register_seller_button"
                    android:layout_width="250dp"
                    android:layout_height="50dp"
                    android:layout_below="@id/et_IFSC_code"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="50dp"
                    android:background="@drawable/button_view_background"
                    android:elevation="10dp"
                    android:gravity="center"
                    android:text="@string/button_register"
                    android:textColor="@color/white"
                    android:textSize="25sp"
                    android:textStyle="bold" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
    </RelativeLayout>
</ScrollView>